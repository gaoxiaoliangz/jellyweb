{
  "name": "example-es-app",
  "version": "0.1.0",
  "scripts": {
    "start": "wparcel src/index.html",
    "start:d": "node -r source-map-support/register ../wparcel/bin/index.js src/index.html",
    "prebuild": "rimraf dist",
    "build": "wparcel build src/index.html",
    "build:d": "node -r source-map-support/register ../wparcel/bin/index.js build src/index.html"
  },
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "rimraf": "^2.6.3",
    "serve": "^11.0.0",
    "wparcel": "0.7.0"
  }
}
